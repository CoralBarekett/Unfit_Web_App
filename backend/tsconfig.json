{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "rootDir": "./src",
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "strict": true,
        "types": ["node", "jest"],
        "skipLibCheck": true
    },
        "include": ["src"],
        "lib": ["es2015"]
}